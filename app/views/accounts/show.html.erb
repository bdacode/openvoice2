<h2>My account page</h2>

<h3>Your number is: <span id="number"><%= current_account.number %></span></h3>

<%= link_to 'Add a new endpoint', new_endpoint_path %>

<ul id="endpoints">
<% current_account.endpoints.each do |endpoint| %>
  <li class='endpoint'><%= endpoint.address %></li>
<% end %>
</ul>